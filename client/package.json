    "lodash": "^4.17.4",
    "moment": "^2.19.2",
    "moment-duration-format": "^2.2.2",
    "node-sass": "^4.7.2",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.6",
    "promise": "7.1.1",
    "prop-types": "^15.5.10",
    "purify-css": "^1.2.5",
    "purifycss-webpack": "^0.7.0",
    "query-string": "^6.1.0",
    "react": "16.6",
    "react-apollo": "^2.1.4",
    "react-datepicker": "^1.2.2",
    "react-dev-utils": "^3.0.2",
    "react-device-detect": "^1.6.1",
