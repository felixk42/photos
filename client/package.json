    "change-case": "^3.1.0",
    "concurrently": "^3.5.0",
    "css-loader": "0.28.4",
    "dotenv": "4.0.0",
    "es6-shim": "^0.35.3",
    "eslint": "4.12.0",
    "eslint-config-react-app": "^2.1.0",
